#include <stdio.h>

int main()
{
    int num1, num2, i, j, flag, temp;
    printf("Enter two numbers(intervals): ");
    scanf("%d %d", &num1, &num2);
    //Swapping the numbers
    if (num2 < num1)
    {
        temp = num1;
        num1 = num2;
        num2 = temp;
    }
    printf("Prime numbers are: ");
    while (num1 < num2)
    {
        flag = 0;
        for (i = 2; i <= num1/2; ++i)
        {
            if (num1 % i == 0)
            {
                flag = 1;
                break;
            }
        }
        if (flag == 0)
            printf("%d ", num1);
        ++num1;
    }
    return 0;
}


the prime numberbetween 70 and 85 =4
